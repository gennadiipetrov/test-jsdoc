{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
                    "./project1",
                    "./project2",
                    "./project3"
                   ],
        "includePattern": "\\.(jsx|js|ts|tsx)$",
        "excludePattern": "(node_modules/|docs|(bundle.*\\.js)|cli.ts)"
    },
    "plugins": [
        "jsdoc-mermaid",
        "node_modules/better-docs/typescript",
        "node_modules/better-docs/category",
        "node_modules/better-docs/component",
        "plugins/markdown"
     ],
    "opts": {
        "template": "node_modules/better-docs",
        "encoding": "utf8",
        "destination": "docs",
        "readme": "./docs-src/Var.md",
        "recurse": true,
        "private": true,
        "verbose": true,
        "tutorials": "./docs-src",
        "sections": "./docs-src"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "search": true,
        "default": {
            "includeDate": false,
            "staticFiles": {
              "include": [
                  "./docs-src"
              ]
            }
        },
        "better-docs": {
            "name": "Region 18 Documentation",
            "logo": "",
            "title": "",
            "css": "style.css",
            "trackingCode": "tracking-code-which-will-go-to-the-HEAD",
            "hideGenerator": false,
                "navLinks": [
                    {
                        "label": "AntiPatterns",
                        "href": "https://sourcemaking.com/antipatterns/software-development-antipatterns"
                    },
                    {
                        "label": "FLEXBOX FROGGY",
                        "href": "https://flexboxfroggy.com/#ru"
                    }
                ]
        }
    }
}